{
  "name": "tinyest",
  "private": false,
  "version": "0.1.1",
  "description": "A compact, fast, and embeddable JavaScript AST for transpilation.",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "module": "./index.js",
      "import": "./index.js",
      "default": "./index.cjs"
    },
    "./*": {
      "types": "./*.d.ts",
      "module": "./*.js",
      "import": "./*.js",
      "default": "./*.cjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/TypeGPU.git"
  },
  "keywords": [
    "ast",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/software-mansion/TypeGPU/issues"
  },
  "homepage": "https://docs.swmansion.com/TypeGPU",
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {},
  "peerDependencies": {},
  "scripts": {}
}