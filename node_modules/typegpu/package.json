{
  "name": "typegpu",
  "private": false,
  "version": "0.6.0",
  "description": "A thin layer between JS and WebGPU/WGSL that improves development experience and allows for faster iteration.",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "module": "./index.js",
      "import": "./index.js",
      "default": "./index.cjs"
    },
    "./data": {
      "types": "./data/index.d.ts",
      "module": "./data/index.js",
      "import": "./data/index.js",
      "default": "./data/index.cjs"
    },
    "./std": {
      "types": "./std/index.d.ts",
      "module": "./std/index.js",
      "import": "./std/index.js",
      "default": "./std/index.cjs"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/TypeGPU.git"
  },
  "keywords": [
    "webgpu",
    "wgpu",
    "wgsl",
    "typescript",
    "compute",
    "shader",
    "shaders",
    "gpgpu"
  ],
  "bugs": {
    "url": "https://github.com/software-mansion/TypeGPU/issues"
  },
  "homepage": "https://typegpu.com",
  "dependencies": {
    "tinyest": "~0.1.1",
    "typed-binary": "^4.3.1"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "peerDependencies": {},
  "scripts": {}
}